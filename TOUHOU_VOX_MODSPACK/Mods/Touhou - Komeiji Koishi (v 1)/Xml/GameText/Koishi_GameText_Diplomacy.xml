		<!-- AI dislikes you -->
		<Row Tag="TXT_KEY_LEADER_KOISHI_GREETING_HOSTILE_HUMAN_AT_WAR_1">
			<Text>Be careful, Miss Mary is searching for you.</Text>
		</Row>
