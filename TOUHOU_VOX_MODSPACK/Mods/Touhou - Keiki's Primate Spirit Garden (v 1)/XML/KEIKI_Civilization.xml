		<Row Tag="TXT_KEY_CIVILIZATION_KEIKI_WORKSHOP_TITLE">
			<Text>Primate Spirit Garden</Text>
		</Row>
		<Row Tag="TXT_KEY_CIVILIZATION_KEIKI_WORKSHOP_SHORT_DESC">
			<Text>Primate Spirit Garden</Text>
